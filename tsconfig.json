{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "declaration": true,
    "sourceMap": true,
    "lib": ["es2017", "esnext.asynciterable", "dom"],
    "rootDir": "./src",
    "outDir": "./build"
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.js",
    "./src/**/*.d.ts",
    "./src/**/scrapers/*.js",
    "./src/**/scrapers/*.d.ts",
    "./src/scrapers/*.js",
    "./src/scrapers/*.d.ts",
    "src/boxdepotet.ts",
    "src/nettolager.ts",
    "src/**/*.ts"
  ]
}
